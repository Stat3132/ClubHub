<html lang="en">
<head><meta charset="UTF-8"><title>Admin Panel</title></head>
<body>
  <h1>Admin Panel</h1>
  <h2>Users</h2>
  <ul>
    {% for user in users %}
    <li>{{user.firstName}} {{user.lastName}} - {{user.role}} <a href="/promote/{{user.userID}}">Promote</a> | <a href="/deleteUser/{{user.userID}}">Delete</a></li>
    {% endfor %}
  </ul>

  <h2>Clubs</h2>
  <ul>
    {% for club in clubs %}
    <li>{{club.clubName}} <a href="/deleteClub/{{club.clubID}}">Delete</a></li>
    {% endfor %}
  </ul>
</body>
</html>